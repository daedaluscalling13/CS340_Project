    <table class='table'>
        <tr>
            <th>Category Name</th>
        </tr>
        {{#each categoryList}}
        <tr>
            <td id='category + {{this.categoryID}}'>{{this.categoryName}}</td>
            <td><button id="editButton{{this.categoryID}}" class='btn btn-primary'>Edit</button></td>
            <td><button id="deleteButton{{this.categoryID}}" class='btn btn-link'>Delete</button></td>
        </tr>
        {{/each}}
    </table>
    <button class='btn btn-link' href='/add_entry'>Cancel</button>
